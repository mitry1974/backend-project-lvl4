{
  "name": "backend-project-lvl4",
  "version": "1.0.0",
  "description": "Hexlet nackend node.js project 4 level",
  "engines": {
    "node": "14.4.0"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx webpack-cli && npx babel server --out-dir dist/server && npx babel dbconfig.js --out-dir dist/ && cp webpack.config.js package.json .sequelizerc dist && cp -r server/views dist/server/ && npm run migrations-run && npm run seeds-run",
    "start": "cd ./dist && node server/bin/server.js",
    "start-production-debug": "cd ./dist && node server/bin/server.js --inspect",
    "start-debug-client": "npx webpack serve --config webpack.config.js",
    "start-debug-server": "npx nodemon --exec DEBUG=application:* npx babel-node -r dotenv/config server/bin/server.js --inspect --source-maps inline",
    "start-debug": "npx concurrently npm:start-debug-client npm:start-debug-server",
    "test": "npx jest",
    "test-coverage": "npx jest --collectCoverage",
    "test-watch": "npx jest --watch",
    "migrations-run": "npx sequelize db:migrate",
    "seeds-run": "npx sequelize db:seed:all"
  },
