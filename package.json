{
  "name": "backend-project-lvl4",
  "version": "1.0.0",
  "description": "Hexlet nackend node.js project 4 level",
  "engines": {
    "node": "13.6.0"
  },
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run migration:compile && npx webpack -p && NODE_ENV=production npx babel server --out-dir dist/server && babel config --out-dir dist/config && cp webpack.config.js package.json dist && cp .env.production dist/.env && cp -r server/views dist/server/",
    "start": "NODE_ENV=production NODE_CONFIG_DIR=./dist/config cd ./dist && node -r dotenv/config server/bin/server.js",
    "start:server:dev": "NODE_ENV=development npx nodemon --exec npx babel-node -r dotenv/config server/bin/server.js",
    "start:frontend:dev": "npx webpack-dev-server",
    "start:dev": "heroku local -f Procfile.dev",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "migrations:run": "npx sequelize db:migrate",
    "seeds:run": "npx sequelize db:seed:all"
  },
  "dependencies": {
    "acorn": "^7.2.0",
    "autoprefixer": "^9.8.0",
    "bootstrap": "^4.5.0",
    "class-transformer": "^0.2.3",
