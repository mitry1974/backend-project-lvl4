    "config": "^3.3.1",
    "connect-flash": "^0.1.1",
    "css-loader": "^3.5.2",
    "dotenv": "^8.2.0",
    "fastify": "^2.14.1",
    "fastify-acl-auth": "^1.4.0",
    "fastify-auth": "^0.7.2",
    "fastify-cookie": "^3.6.0",
    "fastify-error-page": "^3.0.0",
    "fastify-flash": "^1.0.6",
    "fastify-formbody": "^3.2.0",
    "fastify-method-override": "^1.1.0",
    "fastify-reverse-routes": "^1.1.0",
    "fastify-secure-session": "^1.2.0",
    "fastify-sequelize": "^1.0.4",
    "fastify-session": "^3.3.0",
    "fastify-static": "^2.7.0",
    "i18next": "^19.4.5",
    "i18next-node-fs-backend": "^2.1.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "pg": "^8.2.1",
